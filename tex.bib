  issn-linking    = {1471-2148},
  keywords        = {Amino Acid Sequence; Bayes Theorem; Cystine-Knot Miniproteins, genetics; DNA, Plant, genetics; Evolution, Molecular; Fabaceae, genetics; Gene Duplication; Gene Transfer, Horizontal; Genes, Plant; Likelihood Functions; Molecular Sequence Data; Orobanchaceae, genetics; Phylogeny; Protein Structure, Tertiary; Sequence Alignment; Sequence Analysis, DNA},
