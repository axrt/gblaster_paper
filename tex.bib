}

@Article{Zhang2013,
  author          = {Zhang, Yeting and Fernandez-Aparicio, Monica and Wafula, Eric K and Das, Malay and Jiao, Yuannian and Wickett, Norman J and Honaas, Loren A and Ralph, Paula E and Wojciechowski, Martin F and Timko, Michael P and Yoder, John I and Westwood, James H and Depamphilis, Claude W},
  title           = {Evolution of a horizontally acquired legume gene, albumin 1, in the parasitic plant Phelipanche aegyptiaca and related species.},
